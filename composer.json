{
    "name": "nfacha/yii2-log-reader",
    "description": "Yii2 log reader",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","module","log","log reader"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "kriss",
            "email": "462679766@qq.com"
        },
        {
            "name": "Nuno Facha",
            "email": "nuno@facha.dev"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.14"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "nfacha\\logReader\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
